{
  "private": true,
  "name": "muslib",
  "workspaces": ["packages/*"],
  "scripts": {
    "start": "concurrently -k -p \"[{name}]\" -n \"Client,Server\" -c \"green.bold,blue.bold\" \"yarn workspace @muslib/client start\" \"yarn workspace @muslib/server start\"",
    "watch": "concurrently -k -p \"[{name}]\" -n \"Client,Server\" -c \"green.bold,blue.bold\" \"yarn workspace @muslib/client start\" \"yarn workspace @muslib/server watch\""
  },
  "dependencies": {
    "rxjs": "^6.4.0"
  },
  "devDependencies": {
    "typescript": "3.4.5",
    "tslint": "~5.17.0",
    "concurrently": "4.1.0"
  }
}
